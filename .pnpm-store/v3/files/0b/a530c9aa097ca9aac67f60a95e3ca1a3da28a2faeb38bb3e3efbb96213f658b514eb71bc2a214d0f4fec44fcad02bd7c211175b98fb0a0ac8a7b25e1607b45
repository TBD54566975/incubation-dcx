/**
 * Array utility methods.
 */
export declare class ArrayUtility {
    /**
     * Returns `true` if content of the two given byte arrays are equal; `false` otherwise.
     */
    static byteArraysEqual(array1: Uint8Array, array2: Uint8Array): boolean;
    /**
     * Asynchronously iterates an {AsyncGenerator} to return all the values in an array.
     */
    static fromAsyncGenerator<T>(iterator: AsyncGenerator<T>): Promise<Array<T>>;
    /**
     * Generic asynchronous sort method.
     */
    static asyncSort<T>(array: T[], asyncComparer: (a: T, b: T) => Promise<number>): Promise<T[]>;
}
